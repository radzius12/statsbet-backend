{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
// W backendzie na Vercel
app.use(cors({
  origin: [
    'http://statsbet.pl',
    'https://statsbet.pl',
    'http://localhost:3000',
    'http://127.0.0.1:3000'
  ],
  credentials: true
}));
